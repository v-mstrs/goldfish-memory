{
  "manifest_version": 3,
  "name": "goldfish-dexie",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches":["https://wetriedtls.com/*","https://revengernovel.com/*"],
      "js": ["content.js"],
      "type": "module"
    }
  ],
  "action": {
    "default_popup": "index.html" 
  },
  "permissions": ["storage"],
  "browser_specific_settings": {
    "gecko": {
      "id": "goldfish-db@yourproject.com",
      "strict_min_version": "109.0"
    }
  }
}